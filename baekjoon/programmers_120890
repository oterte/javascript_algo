function solution(array, n) {
    var answer = 0;
    //  정렬하기
    array.sort((a, b) => a-b)
    // 차가 가장 작은것 찾기
    let arr = []
    for(let i = 0; i<array.length; i++){
        if(n - array[i] < 0){
            let num = (n - array[i]) * -1;
            arr.push(num)
        }else{
            arr.push(n - array[i])
        }
    }
    
    return array[arr.indexOf(Math.min(...arr))]
}
